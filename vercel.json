{
    "builds": [
        {
            "src": "django_test/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.10"
            }
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": { "distDir": "staticfiles" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "django_test/wsgi.py"
        }
    ]
}