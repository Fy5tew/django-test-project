{
    "version": 2,
    "builds": [
        {
            "src": "django_test/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.10",
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/build/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "django_test/wsgi.py"
        }
    ],
    "outputDirectory": "build/static"
}